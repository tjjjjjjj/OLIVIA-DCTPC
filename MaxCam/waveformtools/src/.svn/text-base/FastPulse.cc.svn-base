#include "FastPulse.hh"

ClassImp(FastPulse)

void
FastPulse::setFastRise(double* v)
{
  fR0 = v[0];
  fR10 = v[1];
  fR25 = v[2];
  fR50 = v[3];
  fR75 = v[4];
  fR90 = v[5];
}

void
FastPulse::setSlowFall(double* v)
{
  sF0 = v[5];
  sF10 = v[4];
  sF25 = v[3];
  sF50 = v[2];
  sF75 = v[1];
  sF90 = v[0];

}

void
FastPulse::setRise(double* v)
{
  R0 = v[0];
  R10 = v[1];
  R25 = v[2];
  R50 = v[3];
  R75 = v[4];
  R90 = v[5];

}

void
FastPulse::setFall(double* v)
{
  F0 = v[5];
  F10 = v[4];
  F25 = v[3];
  F50 = v[2];
  F75 = v[1];
  F90 = v[0];

}
